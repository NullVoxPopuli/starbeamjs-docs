{
  "name": "vitepress-snippets",
  "version": "0.9.0",
  "type": "module",
  "main": "dist/build.cjs",
  "module": "dist/build.js",
  "types": "dist/dts/src/build.d.ts",
  "exports": {
    "./build": {
      "types": "./src/build.ts",
      "import": "./src/build.ts"
    },
    "./browser": {
      "types": "./src/browser.ts",
      "import": "./src/browser.ts"
    }
  },
  "description": "Enhanced JavaScript and TypeScript code snippets for Vitepress",
  "dependencies": {
    "@vueuse/core": "^9.1.1",
    "docs-snippet": "file:./docs-snippet.tgz",
    "markdown-it": "^13.0.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@swc/core": "^1.2.244",
    "@swc/helpers": "^0.4.11",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.7.13"
  },
  "pnpm": {
    "overrides": {
      "typescript": "next"
    }
  }
}
